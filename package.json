{
  "name": "@ycmfe/vue-cli-plugin-cactus-commitlint",
  "version": "1.0.0",
  "description": "cactus commitlint plugin",
  "scripts": {
    "cz": "git add . && git cz"
  },
  "keywords": [
    "vue",
    "cli",
    "cactus",
    "commitlint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycmfe/vue-cli-plugin-cactus-commitlint.git"
  },
  "homepage": "https://github.com/ycmfe/vue-cli-plugin-cactus-commitlint.git#readme",
  "author": "chord",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "husky": "^3.0.9"
  },
  "config": {
    "commitizen": {
      "path": "lib/cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "word-wrap": "^1.2.3"
  }
}
